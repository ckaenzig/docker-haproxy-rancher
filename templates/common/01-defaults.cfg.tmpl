# common/01-defaults.cfg.tmpl

defaults
    log global
    mode http
    option httplog
    timeout connect 5s
    timeout client  20s
    timeout server  30s
    timeout http-request 10s
    timeout http-keep-alive 5s
    timeout check   2s

{{- range getvs "/self/host/labels/lb.haproxy_defaults.*" }}
    {{.}}
{{- end }}

